@misc{paulsen2016noise,
  abstract = {When optimizing the {S}harpe ratio over a k-dimensional parameter space the
thus obtained in-sample {S}harpe ratio tends to be higher than what will be
captured out-of-sample. For two reasons: the estimated parameter will be skewed
towards the noise in the in-sample data (noise fitting) and, second, the
estimated parameter will deviate from the optimal parameter (estimation error).
This article derives a simple correction for both. Selecting a model with the
highest corrected {S}harpe selects the model with the highest expected
out-of-sample {S}harpe in the same way as selection by Akaike Information
Criterion does for the log-likelihood as measure of fit.},
  added-at = {2016-02-27T05:55:48.000+0100},
  author = {Paulsen, Dirk and S\"{o}hl, Jakob},
  biburl = {http://www.bibsonomy.org/bibtex/209741a2c537a7c451941b879078c6365/shabbychef},
  description = {[1602.06186] Noise Fit, Estimation Error and a Sharpe Information Criterion},
  interhash = {52b28cf0e3a2805c1945f8fd1cd5a993},
  intrahash = {09741a2c537a7c451941b879078c6365},
  keywords = {quantfinance sharpe},
  sepnote = {cite arxiv:1602.06186 Comment: 48 pages, 10 figures},
  timestamp = {2016-02-27T05:55:48.000+0100},
  title = {Noise Fit, Estimation Error and a {S}harpe Information Criterion},
  url = {http://arxiv.org/abs/1602.06186},
  year = 2016
}

@article{doi:10.1080/14697688.2020.1718746,
  author = {Paulsen, Dirk and S\"{o}hl, Jakob},
	title = {Noise fit, estimation error and a {S}harpe information criterion},
	journal = {Quantitative Finance},
	volume = {0},
	number = {0},
	pages = {1-17},
	year  = {2020},
	publisher = {Routledge},
	doi = {10.1080/14697688.2020.1718746},
	URL = {https://doi.org/10.1080/14697688.2020.1718746},
	eprint = {https://doi.org/10.1080/14697688.2020.1718746},
	abstract = { When the in-sample Sharpe ratio is obtained by optimizing over a k-dimensional parameter space, it is a biased estimator for what can be expected on unseen data (out-of-sample). We derive (1) an unbiased estimator adjusting for both sources of bias: noise fit and estimation error. We then show (2) how to use the adjusted Sharpe ratio as model selection criterion analogously to the Akaike Information Criterion (AIC). Selecting a model with the highest adjusted Sharpe ratio selects the model with the highest estimated out-of-sample Sharpe ratio in the same way as selection by AIC does for the log-likelihood as a measure of fit. }
}



